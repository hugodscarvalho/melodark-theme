{
	"name": "Melodark Theme",
	"type": "dark",
	"tokenColors": [
	  {
		"name": "New keyword, logical operator keyword",
		"scope": ["keyword.operator.new", "keyword.operator.logical"],
		"settings": {
		  "foreground": "#ba8ef7"
		}
	  },
	  {
		"name": "Comment",
		"scope": ["comment"],
		"settings": {
		  "foreground": "#707a84"
		}
	  },
	  {
		"name": "Comment Shebang",
		"scope": ["comment.line.shebang"],
		"settings": {
		  "foreground": "#ff8ad1"
		}
	  },
	  {
		"name": "Variables",
		"scope": ["variable", "string constant.other.placeholder"],
		"settings": {
		  "foreground": "#939da5"
		}
	  },
	  {
		"name": "Invalid",
		"scope": ["invalid", "invalid.illegal"],
		"settings": {
		  "foreground": "#ff6a80"
		}
	  },
	  {
		"name": "Keyword, Storage",
		"scope": ["keyword", "storage.type", "storage.modifier"],
		"settings": {
		  "foreground": "#ba8ef7"
		}
	  },
	  {
		"name": "Operators",
		"scope": ["keyword.operator"],
		"settings": {
		  "foreground": "#939da5"
		}
	  },
	  {
		"name": "Operator, Misc",
		"scope": ["keyword.control"],
		"settings": {
		  "foreground": "#ba8ef7"
		}
	  },
	  {
		"name": "Constant",
		"scope": ["constant.language", "variable.other.property"],
		"settings": {
		  "foreground": "#ffa763"
		}
	  },
	  {
		"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
		"scope": [
		  "variable.parameter.function.language.special",
		  "variable.parameter"
		],
		"settings": {
		  "foreground": "#FF5370"
		}
	  },	
	  {
		"name": "Punctuation template expression",
		"scope": [
		  "punctuation.definition.template-expression",
		  "punctuation.definition.typeparameters",
		  "meta.template.expression"
		],
		"settings": {
		  "foreground": "#939da5"
		}
	  },
	  {
		"name": "Tag, Misc",
		"scope": [
		  "punctuation.definition.tag",
		  "punctuation.separator.inheritance.php",
		  "punctuation.definition.tag.html",
		  "punctuation.definition.tag.begin.html",
		  "punctuation.definition.tag.end.html"
		],
		"settings": {
		  "foreground": "#939da5"
		}
	  },
	  {
		"name": "Builtin function",
		"scope": ["support.function.builtin"],
		"settings": {
		  "foreground": "#89ddff"
		}
	  },
	  {
		"name": "Tag",
		"scope": [
		  "entity.name.tag",
		  "meta.tag.sgml",
		  "markup.deleted.git_gutter"
		],
		"settings": {
		  "foreground": "#ff6a80"
		}
	  },
	  {
		"name": "Component Tag",
		"scope": ["support.class.component"],
		"settings": {
		  "foreground": "#ba8ef7"
		}
	  },
	  {
		"name": "Function, Special Method",
		"scope": [
		  "entity.name.function",
		  "meta.function-call",
		  "variable.function",
		  "support.function",
		  "keyword.other.special-method"
		],
		"settings": {
		  "foreground": "#ffea6b"
		}
	  },
	  {
		"name": "Function, Special Method",
		"scope": ["punctuation.definition.arguments", "meta.function-call"],
		"settings": {
		  "foreground": "#939da5"
		}
	  },
	  {
		"name": "punctuation.accessor.js",
		"scope": ["punctuation.accessor.js", "punctuation.parenthesis"],
		"settings": {
		  "foreground": "#939da5"
		}
	  },
	  {
		"name": "Block Level Variables",
		"scope": ["variable.other"],
		"settings": {
		  "foreground": "#939da5"
		}
	  },
	  {
		"name": "Number",
		"scope": ["constant.numeric"],
		"settings": {
		  "foreground": "#89ddff"
		}
	  },
	  {
		"name": "Constant, Function Argument, Tag Attribute, Embedded",
